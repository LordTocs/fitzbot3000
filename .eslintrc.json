{
    "extends": ["prettier/recommended", "node/recommended"],
    "rules": {
      "prettier/prettier": "warn",
      "no-unused-vars": "warn",
      "no-console": "off",
      "func-names": "off",
      "no-process-exit": "off",
      "object-shorthand": "off",
      "class-methods-use-this": "off",
      "linebreak-style": ["warn", "windows"]
    }
  }