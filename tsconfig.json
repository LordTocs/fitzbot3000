{
  "compilerOptions": {
    "target": "es5",
    // "module": "es2020",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": "."
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ],
  "types": [
    "node_modules",
  ]
}